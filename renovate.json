{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "baseBranches": ["main"],
  "packageRules": [
    {
      "matchPackagePatterns": [".*"],
      "matchUpdateTypes": ["major", "minor", "patch"]
    }
  ],
  "npm": {
    "rangeStrategy": "bump"
  }
}
