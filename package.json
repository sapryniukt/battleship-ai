{
  "name": "battleship-ai",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "format": "prettier --write \"**/*.{js,ts,json,vue,md,yml}\"",
    "lint": "eslint",
    "test:unit": "vitest run",
    "test:e2e:server": "pnpm preview",
    "test:e2e": "pnpm exec playwright test",
    "test:e2e:chromium": "pnpm exec playwright test --project=chromium",
    "test:e2e:firefox": "pnpm exec playwright test --project=firefox",
    "test:e2e:webkit": "pnpm exec playwright test --project=webkit",
    "test": "pnpm test:unit && pnpm test:e2e"
  },
